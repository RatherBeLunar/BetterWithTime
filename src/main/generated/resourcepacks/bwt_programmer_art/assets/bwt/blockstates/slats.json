{
  "multipart": [
    {
      "apply": {
        "model": "bwt:block/slats_post_ends"
      }
    },
    {
      "apply": {
        "model": "bwt:block/slats_post"
      },
      "when": {
        "east": "false",
        "north": "false",
        "south": "false",
        "west": "false"
      }
    },
    {
      "apply": {
        "model": "bwt:block/slats_cap"
      },
      "when": {
        "east": "false",
        "north": "true",
        "south": "false",
        "west": "false"
      }
    },
    {
      "apply": {
        "model": "bwt:block/slats_cap",
        "y": 90
      },
      "when": {
        "east": "true",
        "north": "false",
        "south": "false",
        "west": "false"
      }
    },
    {
      "apply": {
        "model": "bwt:block/slats_cap_alt"
      },
      "when": {
        "east": "false",
        "north": "false",
        "south": "true",
        "west": "false"
      }
    },
    {
      "apply": {
        "model": "bwt:block/slats_cap_alt",
        "y": 90
      },
      "when": {
        "east": "false",
        "north": "false",
        "south": "false",
        "west": "true"
      }
    },
    {
      "apply": {
        "model": "bwt:block/slats_side"
      },
      "when": {
        "north": "true"
      }
    },
    {
      "apply": {
        "model": "bwt:block/slats_side",
        "y": 90
      },
      "when": {
        "east": "true"
      }
    },
    {
      "apply": {
        "model": "bwt:block/slats_side_alt"
      },
      "when": {
        "south": "true"
      }
    },
    {
      "apply": {
        "model": "bwt:block/slats_side_alt",
        "y": 90
      },
      "when": {
        "west": "true"
      }
    }
  ]
}