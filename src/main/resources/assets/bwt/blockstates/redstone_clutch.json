{
  "multipart": [
    {
      "when": { "north": "true" },
      "apply": { "model": "bwt:block/redstone_clutch_axle_out"}
    },
    {
      "when": { "north": "false" },
      "apply": { "model": "bwt:block/redstone_clutch_side"}
    },
    {
      "when": { "east": "true" },
      "apply": { "model": "bwt:block/redstone_clutch_axle_out", "y": 90}
    },
    {
      "when": { "east": "false" },
      "apply": { "model": "bwt:block/redstone_clutch_side", "y": 90}
    },
    {
      "when": { "south": "true" },
      "apply": { "model": "bwt:block/redstone_clutch_axle_out", "y": 180}
    },
    {
      "when": { "south": "false" },
      "apply": { "model": "bwt:block/redstone_clutch_side", "y": 180}
    },
    {
      "when": { "west": "true" },
      "apply": { "model": "bwt:block/redstone_clutch_axle_out", "y": 270}
    },
    {
      "when": { "west": "false" },
      "apply": { "model": "bwt:block/redstone_clutch_side", "y": 270}
    },
    {
      "when": { "up": "true" },
      "apply": { "model": "bwt:block/redstone_clutch_axle_out", "x": 270}
    },
    {
      "when": { "up": "false" },
      "apply": { "model": "bwt:block/redstone_clutch_side", "x": 270}
    },
    {
      "when": { "down": "true" },
      "apply": { "model": "bwt:block/redstone_clutch_axle_out", "x": 90}
    },
    {
      "when": { "down": "false" },
      "apply": { "model": "bwt:block/redstone_clutch_side", "x": 90}
    },
    {
      "when": { "facing": "north", "mech_powered": "true"},
      "apply": { "model": "bwt:block/redstone_clutch_face_powered_counter_clockwise"}
    },
    {
      "when": { "facing": "east", "mech_powered": "true"},
      "apply": { "model": "bwt:block/redstone_clutch_face_powered_counter_clockwise", "y": 90}
    },
    {
      "when": { "facing": "south", "mech_powered": "true"},
      "apply": { "model": "bwt:block/redstone_clutch_face_powered", "y": 180}
    },
    {
      "when": { "facing": "west", "mech_powered": "true"},
      "apply": { "model": "bwt:block/redstone_clutch_face_powered", "y": 270}
    },
    {
      "when": { "facing": "up", "mech_powered": "true"},
      "apply": { "model": "bwt:block/redstone_clutch_face_powered", "x": 270}
    },
    {
      "when": { "facing": "down", "mech_powered": "true"},
      "apply": { "model": "bwt:block/redstone_clutch_face_powered_counter_clockwise", "x": 90}
    },
    {
      "when": { "facing": "north", "mech_powered": "false"},
      "apply": { "model": "bwt:block/redstone_clutch_face"}
    },
    {
      "when": { "facing": "east", "mech_powered": "false"},
      "apply": { "model": "bwt:block/redstone_clutch_face", "y": 90}
    },
    {
      "when": { "facing": "south", "mech_powered": "false"},
      "apply": { "model": "bwt:block/redstone_clutch_face", "y": 180}
    },
    {
      "when": { "facing": "west", "mech_powered": "false"},
      "apply": { "model": "bwt:block/redstone_clutch_face", "y": 270}
    },
    {
      "when": { "facing": "up", "mech_powered": "false"},
      "apply": { "model": "bwt:block/redstone_clutch_face", "x": 270}
    },
    {
      "when": { "facing": "down", "mech_powered": "false"},
      "apply": { "model": "bwt:block/redstone_clutch_face", "x": 90}
    }
  ]
}

