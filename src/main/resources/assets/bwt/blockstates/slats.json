{
  "multipart": [
    {
      "apply": {
        "model": "bwt:block/slats_post"
      }
    },
    {
      "apply": {
        "model": "bwt:block/slats_side"
      },
      "when": {
        "north": "true"
      }
    },
    {
      "apply": {
        "model": "bwt:block/slats_side",
        "y": 90
      },
      "when": {
        "east": "true"
      }
    },
    {
      "apply": {
        "model": "bwt:block/slats_side_alt"
      },
      "when": {
        "south": "true"
      }
    },
    {
      "apply": {
        "model": "bwt:block/slats_side_alt",
        "y": 90
      },
      "when": {
        "west": "true"
      }
    },
    {
      "apply": {
        "model": "bwt:block/slats_noside"
      },
      "when": {
        "north": "false"
      }
    },
    {
      "apply": {
        "model": "bwt:block/slats_noside_alt"
      },
      "when": {
        "east": "false"
      }
    },
    {
      "apply": {
        "model": "bwt:block/slats_noside_alt",
        "y": 90
      },
      "when": {
        "south": "false"
      }
    },
    {
      "apply": {
        "model": "bwt:block/slats_noside",
        "y": 270
      },
      "when": {
        "west": "false"
      }
    }
  ]
}