{
	"schemaVersion": 1,
	"id": "bwt",
	"version": "${version}",
	"name": "BetterWithTime",
	"description": "This is an example description! Tell everyone what your mod is about!",
	"authors": [
		"RatherBeLunar"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "CC0-1.0",
	"icon": "assets/bwt/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"com.bwt.BetterWithTime"
		],
		"client": [
			"com.bwt.BetterWithTimeClient"
		],
		"fabric-datagen": [
			"com.bwt.BetterWithTimeDataGenerator"
		]
	},
	"accessWidener" : "bwt.accesswidener",
	"mixins": [
		"betterwithtime.mixins.json",
		{
			"config": "betterwithtime.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.15.6",
		"minecraft": "~1.20.4",
		"java": ">=17",
		"fabric-api": ">=0.97.0+1.20.4"
	},
	"suggests": {
		"another-mod": "*"
	}
}